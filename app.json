{
  "expo": {
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 35,
            "targetSdkVersion": 35,
            "buildToolsVersion": "35.0.0"
          }
        }
      ]
    ],
    "name": "Digital House",
    "slug": "digital-house",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo_digital_house.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": false,
    "splash": {
      "image": "./assets/logo_digital_house.png",
      "resizeMode": "contain",
      "backgroundColor": "#0B1220"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "Digital House needs access to your photos so you can choose images and videos for posts.",
        "NSMicrophoneUsageDescription": "Digital House may use the microphone when recording video.",
        "NSCameraUsageDescription": "Digital House needs camera access to take photos or videos for posts.",
        "ITSAppUsesNonExemptEncryption": false
      },
      "bundleIdentifier": "com.anonymous.digital-house"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo_digital_house.png",
        "backgroundColor": "#0B1220"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": [
        "android.permission.READ_MEDIA_IMAGES",
        "android.permission.READ_MEDIA_VIDEO",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.CAMERA",
        "android.permission.READ_MEDIA_IMAGES",
        "android.permission.READ_MEDIA_VIDEO",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.CAMERA"
      ],
      "package": "com.thisisgowtham.digitalhouse"
    },
    "web": {
      "favicon": "./assets/logo_digital_house.png"
    },
    "extra": {
      "eas": {
        "projectId": "defefaa3-71d1-495c-98f8-2489497c65ed"
      }
    },
    "runtimeVersion": "1.0.0",
    "updates": {
      "url": "https://u.expo.dev/defefaa3-71d1-495c-98f8-2489497c65ed",
      "enabled": false
    }
  }
}
